				"pattern": "filter1",
				"targetAggregation": "pages",
				"view": "filter1",
				"controlID": "app"
			}, {
				"name": "filter2",
				"pattern": "filter2",
				"targetAggregation": "pages",
				"view": "filter2",
				"controlID": "app"
